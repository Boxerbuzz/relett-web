
# Hedera Network Configuration
# Get these from https://portal.hedera.com/
HEDERA_NETWORK=testnet
HEDERA_ACCOUNT_ID=0.0.YOUR_ACCOUNT_ID
HEDERA_PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE

# Hedera Testnet Configuration (for testing)
# You can create free testnet accounts at https://portal.hedera.com/
HEDERA_TESTNET_ACCOUNT_ID=0.0.YOUR_TESTNET_ACCOUNT_ID
HEDERA_TESTNET_PRIVATE_KEY=YOUR_TESTNET_PRIVATE_KEY_HERE

# Blockchain Configuration
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/YOUR_PROJECT_ID
POLYGON_RPC_URL=https://polygon-mainnet.g.alchemy.com/v2/YOUR_API_KEY

# Smart Contract Addresses (will be populated after deployment)
PROPERTY_TOKEN_CONTRACT_ADDRESS=
LAND_REGISTRY_CONTRACT_ADDRESS=

# External API Keys for Property Valuation
GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY
NIGERIAN_LAND_REGISTRY_API_KEY=YOUR_REGISTRY_API_KEY

# File Storage Configuration (for document storage)
IPFS_GATEWAY_URL=https://gateway.pinata.cloud/ipfs/
PINATA_API_KEY=YOUR_PINATA_API_KEY
PINATA_SECRET_API_KEY=YOUR_PINATA_SECRET_KEY

# Revenue Distribution Configuration
PAYMENT_PROCESSOR_API_KEY=YOUR_PAYMENT_API_KEY
BANK_API_KEY=YOUR_BANK_API_KEY

# Notification Services
TWILIO_ACCOUNT_SID=YOUR_TWILIO_SID
TWILIO_AUTH_TOKEN=YOUR_TWILIO_TOKEN
SENDGRID_API_KEY=YOUR_SENDGRID_KEY

# Security & Encryption
JWT_SECRET=your-super-secret-jwt-key-here
ENCRYPTION_KEY=your-32-character-encryption-key

# Development vs Production
NODE_ENV=development
